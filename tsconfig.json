{
    "compilerOptions": {
       "target": "ES6",
       "module": "commonjs",
       "strict": true,
       "esModuleInterop": true,
       "skipLibCheck": true,
       "forceConsistentCasingInFileNames": true,
       "baseUrl": ".",
       "paths": {
          "@pages/*": ["pages/*"],
          "@fixtures/*": ["fixtures/*"],
          "@utils/*": ["utils/*"]
       }
    },
    "include": ["**/*.ts"]
 }
 